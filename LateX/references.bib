@article{wildland,
  title = {Wildland Fire Behaviour Case Studies and Fuel Models for Landscape-Scale Fire Modeling},
  author = {Santoni, Paul-Antoine and Filippi, Jean-Baptiste and Balbi, Jacques-Henri and Bosseur, Frédéric},
  year = {2011},
  journal = {Journal of Combustion},
  volume = {2011},
  number = {1},
  eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1155/2011/613424},
  pages = {613424},
  doi = {10.1155/2011/613424},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1155/2011/613424},
  abstract = {This work presents the extension of a physical model for the spreading of surface fire at landscape scale. In previous work, the model was validated at laboratory scale for fire spreading across litters. The model was then modified to consider the structure of actual vegetation and was included in the wildland fire calculation system Forefire that allows converting the two-dimensional model of fire spread to three dimensions, taking into account spatial information. Two wildland fire behavior case studies were elaborated and used as a basis to test the simulator. Both fires were reconstructed, paying attention to the vegetation mapping, fire history, and meteorological data. The local calibration of the simulator required the development of appropriate fuel models for shrubland vegetation (maquis) for use with the model of fire spread. This study showed the capabilities of the simulator during the typical drought season characterizing the Mediterranean climate when most wildfires occur.}
}

