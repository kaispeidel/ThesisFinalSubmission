@article{emimiCurrentOpportunitiesChallenges2023,
  title = {The {{Current Opportunities}} and {{Challenges}} in {{Drone Technology}}},
  author = {Emimi, Mohamed and Khaleel, Mohamed and Alkrash, Abobakr},
  year = {2023},
  journal = {International Journal of Electrical Engineering and Sustainability},
