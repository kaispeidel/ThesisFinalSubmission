@inproceedings{alkhatibProposedAutomaticForest2024,
  title = {A {{Proposed Automatic Forest Fire Extinguishing}} and {{Prevention System Using Drones}}},
  booktitle = {2024 {{IEEE International Humanitarian Technologies Conference}} ({{IHTC}})},
  author = {Alkhatib, Ahmad AA and Mohammad Jaber, Khalid and Al-Madi, Mohammad},
  year = {2024},
  date = {2024-11},
  pages = {1--6},
  issn = {2837-4800},
  doi = {10.1109/IHTC61819.2024.10855034},
  url = {https://ieeexplore.ieee.org/abstract/document/10855034},
  urldate = {2025-04-21},
  abstract = {Fires represent one of the most predominant and destructive disasters, which tend to be rapidly increasing and nearly impossible to control. Conventional firefighting methods, which rely on human intervention, are normally time-consuming and unsafe. The implementation of drones for fire prevention and extinguishing provides an efficient substitute, permitting rapid and safe access to areas, which are inaccessible to human firefighters. Building on the successful completion of our previous project with the Department of Civil Defense, entitled "System for Detection, Prediction, and Analysis using IoT," concluded in December 2022, we propose the second part to create a new initiative: "Automatic Fire Extinguishing and Prevention using Drones." This project aims to enhance an automated system that can detect and extinguish fires in real-time, and thus reduce damage to properties and save lives. This system aims to leverage drone technology by developing the efficiency and safety of fire management operations. Drones equipped with thermal cameras, sensors, and fire-suppressant delivery mechanisms are used to detect fire outbreaks early and respond instantly. These drones can access severe areas that are difficult for human firefighters to approach, by providing a more rapid and safer means of combating fires. By integrating real-time data analysis and autonomous navigation, this system allows incessant monitoring and rapid response to different fire incidents. Eventually, the deployment of drones for automatic fire extinguishing and prevention can revolutionise conventional firefighting methods by producing significant improvement in protecting lives and property from the devastating impacts of fires.},
  eventtitle = {2024 {{IEEE International Humanitarian Technologies Conference}} ({{IHTC}})},
  keywords = {Automatic Extinguishing,Data analysis,Disasters,Drones,Fire Detection,Fire Prediction,Fire Prevention,Forest Fire,Forestry,Monitoring,Prevention and mitigation,Protection,Real-time systems,Thermal sensors,Wireless sensor networks,WSN},
  file = {/Users/kaispeidel/Zotero/storage/QUCDITX7/Alkhatib et al. - 2024 - A Proposed Automatic Forest Fire Extinguishing and Prevention System Using Drones.pdf}
}


